{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "common-ui": [
        "dist/common-ui"
      ],
      "common-ui/*": [
        "dist/common-ui/*"
      ],
      "common-service": [
        "dist/common-service"
      ],
      "common-service/*": [
        "dist/common-service/*"
      ],
      "common-inputs": [
        "dist/common-inputs"
      ],
      "common-inputs/*": [
        "dist/common-inputs/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}